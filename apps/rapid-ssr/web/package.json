{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "pnpm build:webpack:ssr && pnpm build:client",
    "build:webpack:ssr": "npx webpack --config ./webpack.ssr.js --progress",
    "build:webpack:watch": "npx webpack --config ./webpack.ssr.js --progress",
    "build:vite:watch": "vite build --watch --outDir dist/client",
    "build:client": "vite build --outDir dist/client",
    "preview": "vite preview",
    "pretty": "prettier --write \"./**/*.{ts,tsx,json}\"",
    "build:server:vite": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:esbuild": "esbuild src/entry-server.tsx --bundle --platform=browser --outfile=dist/out.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@rapid-web/ui": "^0.0.7",
    "@tanstack/react-query": "^4.19.1",
    "@tanstack/react-query-devtools": "^4.19.1",
    "axios": "^1.2.0",
    "framer-motion": "^7.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.11.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-helmet": "^6.1.6",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "esbuild": "^0.17.4",
    "esbuild-loader": "^2.21.0",
    "file-loader": "^6.2.0",
    "ifdef-loader": "^2.3.2",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "sb": "^6.5.14",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^4.0.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  }
}
